{
  "name": "wikijs-mcp",
  "version": "1.0.0",
  "description": "MCP для работы с Wiki.js GraphQL API",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "demo": "ts-node src/demo.ts",
    "client": "node mcp_client.js",
    "server:stdio": "node mcp_wikijs_stdin.js",
    "server:http": "node mcp_http_server.js",
    "http-client": "node mcp_http_client.js"
  },
  "keywords": [
    "wikijs",
    "graphql",
    "mcp",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "fastify": "^4.27.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "type": "module"
}
